{
  "name": "click-n-wash-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Backend for Click N Wash",
  "main": "index.js",
  "scripts": {
    "test": "ts-mocha src/**/*.spec.ts --exit",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "start": "node ./dist/index.js",
    "start:background": "pm2 start ./dist/index.js --name click-n-wash-backend -f",
    "production": "npm run clean && npm run build && npm run start:background",
    "seed": "tsc && node ./dist/seed.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://code.fbi.h-da.de/click-n-wash/click-n-wash"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "eslint",
    "prettier",
    "mocha",
    "chai"
  ],
  "author": "Bernhard, Moses, Nic, Vinsky, Vinston",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.10.3",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  }
}
